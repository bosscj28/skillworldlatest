<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.shanks.myapp.fragment.FragmentAdd">
    <!-- TODO: Update blank fragment layout -->
   <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:orientation="vertical"
        tools:context=".registration_flow.RegistrationBankDetails">
        <RelativeLayout
            android:id="@+id/activity_registration_bank_details"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/activity_vertical_margin"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:paddingTop="@dimen/activity_vertical_margin">
            <Button
                android:id="@+id/submit1"
                style="?android:textAppearanceSmall"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#0a0762"
                android:textColor="#fff"
                android:layout_marginTop="@dimen/activity_vertical_margin"
                android:gravity="center"
                android:layout_alignParentBottom="true"
                android:padding="1dp"
                android:textSize="14sp"
                android:text="Submit"
                android:textStyle="normal" />
            <LinearLayout
                android:orientation="vertical"
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true">

                <android.support.design.widget.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/view34"
                    android:layout_below="@+id/view1">
                    <AutoCompleteTextView
                        android:id="@+id/ac_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Name"
                        android:inputType="textCapSentences"
                        android:imeOptions="actionNext"
                        android:nextFocusForward="@+id/branchname"
                        android:textSize="14sp"
                        android:maxLines="1"
                        android:maxLength="20"
                        android:layout_marginTop="10dp"
                        android:singleLine="true"/>
                </android.support.design.widget.TextInputLayout>


                <!--<android.support.design.widget.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/view1"
                    android:layout_marginTop="50dp">
                    <AutoCompleteTextView
                        android:id="@+id/bankname"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Bank Name"
                        android:textSize="14sp"
                        android:inputType="textCapSentences"
                        android:imeOptions="actionNext"
                        android:nextFocusForward="@+id/acntno"
                        android:focusableInTouchMode="true"
                        android:maxLines="1"
                        android:maxLength="20"
                        android:layout_marginTop="10dp"
                        android:singleLine="true"/>
                </android.support.design.widget.TextInputLayout>-->

                <android.support.design.widget.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/view2"
                    android:layout_below="@+id/view1">
                    <AutoCompleteTextView
                        android:id="@+id/ac_phonenumber"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Mobile Number"
                        android:inputType="number"
                        android:imeOptions="actionNext"
                        android:nextFocusForward="@+id/branchname"
                        android:textSize="14sp"
                        android:maxLines="1"
                        android:maxLength="10"
                        android:layout_marginTop="10dp"
                        android:singleLine="true"/>
                </android.support.design.widget.TextInputLayout>

                <android.support.design.widget.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/view3"
                    android:layout_below="@+id/view2">
                    <AutoCompleteTextView
                        android:id="@+id/ac_email"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Email"
                        android:inputType="textEmailAddress"
                        android:maxLines="1"
                        android:imeOptions="actionNext"
                        android:nextFocusForward="@+id/ifsc"
                        android:textSize="14sp"
                        android:maxLength="20"
                        android:layout_marginTop="10dp"
                        android:singleLine="true"/>
                </android.support.design.widget.TextInputLayout>
                <TextView
                    android:id="@+id/tv_City"
                    android:text=" City"
                    android:textSize="14sp"
                    android:layout_marginTop="15dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/state_id"
                    android:layout_centerHorizontal="true">
                </TextView>
                <Spinner
                    android:id="@+id/sp_citi"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_below="@+id/tv_City"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />
                <TextView
                    android:id="@+id/tv_Category"
                    android:text=" Category"
                    android:layout_marginTop="15dp"
                    android:textSize="14sp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/state_id"
                    android:layout_centerHorizontal="true">
                </TextView>
                <Spinner
                    android:id="@+id/sp_categor"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:layout_below="@+id/tv_City"
                    android:layout_alignParentRight="true"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentStart="true" />
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>
</FrameLayout>
